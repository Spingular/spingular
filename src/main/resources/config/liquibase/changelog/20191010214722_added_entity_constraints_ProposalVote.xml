    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ProposalVote.
    -->
    <changeSet id="20191010214722-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="appuser_id"
                                 baseTableName="proposal_vote"
                                 constraintName="fk_proposal_vote_appuser_id"
